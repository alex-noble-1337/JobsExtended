  <Item name="" identifier="faradayartifact" category="Alien" Tags="alien,alienartifact" sonarsize="50" scale="0.5" impactsoundtag="impact_metal_heavy" allowsellingwhenbroken="true" hideconditionbar="true">
    <Upgrade gameversion="0.9.3.1" scale="0.5"/>
